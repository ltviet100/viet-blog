<h2 class="text-center">All Categories</h2>
<div align="center">
  <% @categories.each do |category| %>
    <ul class="listing">
      <div class="row">
        <div class="col-md-4 col-md-offset-4 well">
          <%= link_to category.name, category_path(category) %>
        </div>
      </div>
    </ul>
  <% end %>
  <%= will_paginate %>
</div>
